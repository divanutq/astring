{
  "name": "astring",
  "version": "0.8.0",
  "description": "JavaScript code generator from an ESTree-compliant AST.",
  "main": "./dist/astring.js",
  "bin": {
    "astring": "./bin/astring"
  },
  "scripts": {
    "test": "tap --harmony test/index.js",
    "test-scripts": "tap --harmony test/scripts.js",
    "prepublish": "babel src/astring.js --out-file dist/astring.min.js --presets=babili",
    "build": "babel src/astring.js --out-file dist/astring.js",
    "start": "babel src/astring.js --watch --out-file dist/astring.js --source-maps inline",
    "benchmark": "node ./test/benchmark.js"
  },
  "keywords": [
    "ast",
    "code generator",
    "estree"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/davidbonnet/astring.git"
  },
  "author": "David Bonnet <david@bonnet.cc>",
  "license": "MIT",
  "devDependencies": {
    "acorn": "^4.0.11",
    "astravel": "^0.3.11",
    "babel-cli": "^6.24.0",
    "babel-plugin-transform-es2015-modules-umd": "^6.24.0",
    "babel-preset-babili": "0.0.12",
    "babel-preset-latest": "^6.24.0",
    "benchmark": "^2.0.0",
    "eslint": "^3.18.0",
    "eslint-plugin-import": "^2.2.0",
    "glob": "^7.1.1",
    "normalize-newline": "^3.0.0",
    "string.prototype.repeat": "^0.2.0",
    "tap": "^10.3.0"
  },
  "optionalDependencies": {
    "esast": "mason-lang/esast",
    "escodegen": "^1.6.1",
    "esotope": "^1.4.5",
    "uglify-js": "^2.6.0"
  }
}
